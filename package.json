{
  "name": "@nabh/ts-api-extractor",
  "description": "Generates API metadata from Typescript type definitions",
  "version": "1.1.0",
  "main": "dist/dist-cjs/index.js",
  "module": "dist/dist-es/index.js",
  "types": "dist/dist-types/index.d.ts",
  "bin": {
    "ts-api-extractor": "bin/ts-api-extractor.js"
  },
  "scripts": {
    "extract": "api-extractor run --local --verbose",
    "build": "npm run clean && create_index_ts src && npm run build:cjs && npm run build:es && npm run build:types && npm run extract && npm run build:docs",
    "build:cjs": "tsc -p tsconfig.json",
    "build:es": "tsc -p tsconfig.es.json",
    "build:types": "tsc -p tsconfig.types.json",
    "build:docs": "npm run clean:docs && api-documenter markdown --input-folder ./config --output-folder ./docs",
    "clean": "npm run clean:dist && npm run clean:docs",
    "clean:dist": "rimraf ./dist",
    "clean:docs": "rimraf ./docs",
    "test": "jest",
    "lint": "npx eslint --fix src/**"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pdabke/ts-api-extractor.git"
  },
  "keywords": [
    "Typescript",
    "API",
    "Signature"
  ],
  "author": "Padmanabh Dabke",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pdabke/ts-api-extractor/issues"
  },
  "homepage": "https://github.com/pdabke/ts-api-extractor#readme",
  "dependencies": {
    "@microsoft/api-extractor": "^7.19.4",
    "@microsoft/api-extractor-model": "^7.15.3"
  },
  "devDependencies": {
    "@microsoft/api-documenter": "^7.15.1",
    "@nabh/index-ts-creator": "^1.0.1",
    "@types/node": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.7.0",
    "jest": "^27.4.7",
    "rimraf": "^3.0.2"
  }
}
